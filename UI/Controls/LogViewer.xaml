<UserControl x:Class="BananaMacro.UI.Controls.LogViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Grid Background="{DynamicResource BgBrush}" Padding="8">
    <ListBox ItemsSource="{Binding Logs.Lines}" Foreground="{DynamicResource TextBrush}" Background="{DynamicResource PanelBrush}"/>
  </Grid>
</UserControl>