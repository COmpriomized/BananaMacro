<UserControl x:Class="BananaMacro.UI.Views.LogsTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ctrl="clr-namespace:BananaMacro.UI.Controls">
  <Grid Background="{DynamicResource PanelBrush}" Padding="12">
    <StackPanel>
      <TextBlock Text="Logs" Style="{StaticResource SectionTitle}"/>
      <ctrl:LogViewer/>
    </StackPanel>
  </Grid>
</UserControl>